<% if @user!=nil %>

  Welcome <%= @user.user_name %>!

  <form method='post' action='/sessions'>
       <input type='hidden' name='_method' value='delete'>
       <input type='submit' value='sign out'>
     </form>

<% else %>

  Welcome, stranger!


  <form action="/users/new" method="get">
  <input type="submit" value="sign up">
</form>
<%end%>
